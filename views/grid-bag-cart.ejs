    <script type="text/javascript">
        var aray = JSON.parse(window.sessionStorage.getItem("cart"));
        console.log(aray);
        // assign array js to html table
        aray.map((e) => {
            var row = "<tr>\n" +
                "                            <th scope=\"row\" class=\"img-table\">\n" +
                "                                <div class=\"fix-imgs\" style=\"background-image: url('"+e.image+"')\"></div>\n" +
                "                            </th>\n" +
                "                            <td class=\"strong\">"+e.name+"</td>\n" +
                "                            <td style=\"justify-content: center;display: flex\">\n" +
                "                                <input class=\"quantity\" min=\"1\" max=\"15\" name=\"soLuong\" value=\""+e.quantity+"\" type=\"number\" required>\n" +
                "                            </td>\n" +
                "                            <td class=\"strong text-lowercase\"><span class=\"Price\">"+e.price+"</span> k</td>\n" +
                "                            <td class=\"strong text-lowercase\"><span class=\"Subtotal\">"+((e.quantity)*(e.price)) +"</span> k</td>\n" +
                "                            <td class=\"strong\">\n" +
                "                                <button type=\"button\" class=\"tag-a btn-outline-yellow\" style=\"margin-bottom: 10px\">Update</button>\n" +
                "                                <button type=\"button\" class=\"tag-a btn-outline-black\">Remove</button>\n" +
                "                            </td>\n" +
                "                          </tr>";

            document.getElementById("myBody").innerHTML += row;
        });
// handle

    </script>

