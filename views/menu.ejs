<!DOCTYPE html>
<html lang="en">

<head>
    <title>Menu</title>
    <%- include('shared-imports') %>
    <link rel="stylesheet" href="/css/menu.css">
    <link rel="stylesheet" href="/css/jquery-yys-slider-old.css">
</head>

<body>
    <%- include('header')%>
    <!-- head -->
    <!--header-->
    <div class="parallax" data-parallax="scroll" data-image-src="/images/rs1.jpg" data-position-Y="bottom">
        <div class="background-color">
            <div class="container">
                <div class="row fix-title-head">
                    <div class="col-md-12 wow  fadeIn" data-wow-delay="0.5s" style="width: 100%;text-align: center">
                        <!--                        data-wow-deley: thời gian chờ chuyển động lại-->
                        <span class="title-yellow-head">Our Menu</span>
                    </div>
                    <div class="col-md-12 wow  bounceIn" data-wow-delay="1s" style="width: 100%;text-align: center">
                        <h2 class=" title-h2-head ">FOOD & DRINKS</h2>
                    </div>
                    <div class="col-md-12 wow  fadeInUp" data-wow-delay="1.5s" style="width: 100%;text-align: center">
                        <p class="p-head">
                            An unforgettable dining experience
                            in the heart of Detech Tower, Vietnam. <br>
                            Contemporary Italian fusion cusine, inspired by the
                            traditions of the rural town Naples.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- menu  -->
    <section class="notice" style="display: none;">
        <div class="container">
            <div class="col-md-12">
                <h4>You have <span id="quantity"></span> dish in <a href="/cart">your cart!</a></h4>
            </div>
        </div>
    </section>

    <section class="section-white">
        <div class="container">
            <div class="row">
                <%- include('grid-menu') %>
            </div>
        </div>
    </section>
    <!--    section parallax-->
    <section class="parallax" data-parallax="scroll" data-image-src="/images/bg8.jpg">
        <div class="background-color">
            <div class="container">
                <div class="row">
                    <div class="col-md-7" style="margin-top:100px ">
                        <div class="text-center margin-bottom-30">
                            <span class="comic-text">Best Recipes</span>
                            <h3 class="section-title white-text">AWARDED CHEFS</h3>
                        </div>
                        <div class="row">
                            <!--begin col-md-6-->

                            <div class="col-md-6">
                                <!--begin menu-wrapper-->
                                <div class="menu-wrapper">
                                    <!--begin home-menu-list-->
                                    <div class="home-menu-list">
                                        <h5 class="white-text">English Breakfast</h5>
                                        <p class="price">$12.95</p>
                                        <span class="dot-line"></span>
                                    </div>

                                    <!--end home-menu-list-->
                                    <p class="home-menu-ingredients">Scrambled eggs with crispy bacon, sausage, black
                                        pudding, tomato &amp; mushroom.</p>
                                </div>
                                <!--end menu-wrapper-->
                            </div>
                            <div class="col-md-6">
                                <!--begin menu-wrapper-->
                                <div class="menu-wrapper">
                                    <!--begin home-menu-list-->
                                    <div class="home-menu-list">
                                        <h5 class="white-text">Lasagna Classica</h5>
                                        <p class="price">$19.55</p>
                                        <span class="dot-line"></span>
                                    </div>
                                    <!--end home-menu-list-->
                                    <p class="home-menu-ingredients">Broccoli rabe, ittalian sausage, peperoncino,
                                        tomates, flour and roasted garlic.</p>
                                </div>
                                <!--end menu-wrapper-->
                            </div>
                            <div class="col-md-6">
                                <!--begin menu-wrapper-->
                                <div class="menu-wrapper">
                                    <!--begin home-menu-list-->
                                    <div class="home-menu-list">
                                        <h5 class="white-text">Vegetables Soup</h5>
                                        <p class="price">$9.95</p>
                                        <span class="dot-line"></span>
                                    </div>
                                    <!--end home-menu-list-->
                                    <p class="home-menu-ingredients">Papaya salsa, sliced red radishes, avocado, corn
                                        tortilla, spicy tomato ketchup.</p>
                                </div>
                                <!--end menu-wrapper-->
                            </div>
                            <div class="col-md-6">
                                <!--begin menu-wrapper-->
                                <div class="menu-wrapper">
                                    <!--begin home-menu-list-->
                                    <div class="home-menu-list">
                                        <h5 class="white-text">Toffee Pudding</h5>
                                        <p class="price">$10.50</p>
                                        <span class="dot-line"></span>
                                    </div>

                                    <!--end home-menu-list-->
                                    <p class="home-menu-ingredients">Baked caramel colored sponge cake with raisins,
                                        ladled with warm toffee sauce.</p>
                                </div>
                                <!--end menu-wrapper-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--slider-->
    <section class="fix-slider" style="width: 100%;margin: 50px 0;">
        <div class="container">
            <div class="content-part part-tese">
                <div class="content-title">
                    <h1>Chef's Pick</h1>
                </div>
                <div class="sw"></div>
                <div class="gallery_container"></div>
            </div>
        </div>
    </section>
    <!--    end slider-->
    <%- include('footer')%>

    <!--    javascript-->
    <script src="https://cdn.bootcss.com/jquery/1.11.3/jquery.min.js"></script>
    <script type="text/javascript" src="/lbs/mdBootstrap/js/parallax.min.js"></script>
    <script type="text/javascript" src="/lbs/mdBootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/lbs/mdBootstrap/js/mdb.min.js"></script>
    <script src="/js/jquery-yys-slider-old.js"></script>
    <script type="text/javascript">
        $(function () {
            $('.content-part.part-tese').addClass('show');
            $('.gallery_container').gallery_slider({
                imgNum: 7,
                imgArr: [
                    '/images/pizzas/4.jpg',
                    '/images/pizzas/5.jpg',
                    '/images/pizzas/1.jpg',
                    '/images/pizzas/2.jpg',
                    '/images/pizzas/3.jpg',
                    '/images/pizzas/6.jpg',
                    '/images/pizzas/7.jpg'
                ]
            });
        })
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            new WOW().init();
        });
    </script>
</body>

</html>